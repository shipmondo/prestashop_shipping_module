{
  "name": "shipmondo/prestashop_shipping_module",
  "description": "A complete shipping solution for PrestaShop",
  "version": "2.2.0",
  "homepage": "https://shipmondo.com",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Shipmondo",
      "email": "support@shipmondo.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/shipmondo/prestashop_shipping_module"
    }
  ],
  "require": {
    "php": ">=7.2"
  },
  "autoload": {
    "psr-4": {
      "Shipmondo\\": "src/"
    }
  },
  "config": {
    "prepend-autoloader": false
  },
  "type": "prestashop-module",
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.92",
    "phpstan/phpstan": "^2.1",
    "prestashop/php-dev-tools": "^5.0",
    "stancer/php-stubs-prestashop": "^8.2"
  }
}
